{
  "name": "express-bookshelf-realworld-example-app",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "author": "Tane Morgan (https://github.com/tanem)",
  "description": "RealWorld example app using Express and Bookshelf.",
  "scripts": {
    "lint": "eslint .",
    "test": "bin/test.sh",
    "start": "bin/start.sh",
    "db:truncate": "bin/truncate.js",
    "docker:db:truncate": "docker-compose exec node npm run db:truncate",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml run node npm run test",
    "docker:start": "docker-compose -f docker-compose.yml -f docker-compose.development.yml up"
  },
  "dependencies": {
    "async-error-catcher": "1.2.0",
    "bcrypt": "3.0.6",
    "body-parser": "1.19.0",
    "bookshelf": "0.15.1",
    "boom": "7.3.0",
    "checkit": "0.7.0",
    "convict": "5.0.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-interceptor": "1.2.0",
    "express-pino-logger": "4.0.0",
    "helmet": "3.18.0",
    "jsonwebtoken": "8.5.1",
    "knex": "0.19.0",
    "lodash": "4.17.14",
    "passport": "0.4.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "7.11.0",
    "pino": "5.12.6",
    "pino-pretty": "3.2.0",
    "require-directory": "2.1.1",
    "serve-favicon": "2.5.0",
    "slug": "1.1.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "add-matchers": "0.6.2",
    "chance": "1.0.18",
    "coveralls": "3.0.5",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-jest": "22.7.2",
    "eslint-plugin-prettier": "3.1.0",
    "jest": "24.8.0",
    "moment": "2.24.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "supertest": "4.0.2"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/_coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/config/"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "globalSetup": "<rootDir>/test-support/global-setup.js",
    "globalTeardown": "<rootDir>/test-support/global-teardown.js",
    "setupFilesAfterEnv": ["<rootDir>/test-support/setup-test-framework.js"],
    "testEnvironment": "<rootDir>/test-support/test-environment.js"
  }
}
